check process logstash-cf
  with pidfile /var/vcap/sys/run/logstash-cf/logstash-cf.pid
  start program "/var/vcap/jobs/logstash-cf/bin/monit_debugger cf_ctl '/var/vcap/jobs/logstash-cf/bin/cf_ctl start'"
  stop program "/var/vcap/jobs/logstash-cf/bin/monit_debugger cf_ctl '/var/vcap/jobs/logstash-cf/bin/cf_ctl stop'"
  group vcap
